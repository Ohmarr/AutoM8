{
	"manifest_version": 2,
	"name": "Code Tools",
	"version": "1.0",
	"description": "A Chrome extension to enhance code-related tasks.",
	"permissions": [
	  "clipboardRead",
	  "downloads",
	  "activeTab"
	],
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": [
		  "contentScript.js"
		]
	  }
	],
	"background": {
	  "scripts": [
		"background.js"
	  ],
	  "persistent": false
	},
	"icons": {
	  "128": "icon128.png"
	}
  }
  